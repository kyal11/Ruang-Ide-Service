model users {
  id           String   @id @default(uuid())
  name         String
  email        String   @unique
  password     String
  nim          String   @unique
  phoneNumber  String
  verified     Boolean  @default(false)
  imageUrl      String?
  role         Role  @default(user)
  bookings     bookings[]
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

enum Role {
  superadmin
  admin
  user
}